kallisto quant -i transcripts.idx -o kallisto/2018.exp1.I1/ -b 100 /data/BurgessLab/sequencing/2018_00_scRNAseq-Graham/2_fastqs/exp1-3K/3K_S1_L001_I1_001.fastq.gz /data/BurgessLab/sequencing/2018_00_scRNAseq-Graham/2_fastqs/exp1-3K/3K_S1_L002_I1_001.fastq.gz

