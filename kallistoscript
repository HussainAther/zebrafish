kallisto quant -i transcripts.idx -o kallisto/2018.exp3.R2/ -b 100 /data/BurgessLab/sequencing/2018_00_scRNAseq-Graham/2_fastqs/exp3-3K/3K_S1_L001_R2_001.fastq.gz /data/BurgessLab/sequencing/2018_00_scRNAseq-Graham/2_fastqs/exp3-3K/3K_S1_L002_R2_001.fastq.gz
